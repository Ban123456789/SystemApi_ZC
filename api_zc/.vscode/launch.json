{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Accura MES API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net6.0/Accura_MES.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_DETAILEDERRORS": "true",
                "ASPNETCORE_DEVELOPEREXCEPTIONPAGE": "true"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Attach to Accura MES API",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}